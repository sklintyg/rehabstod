{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Schema for one patient's sjukfall as resturned from e.g /sjukfall/patient",

    "properties": {
      "sjfMetaData": {
        "properties": {
          "vardenheterInomVGMedSparr": {
            "minItems": 0,
            "type": "array"
          },
          "andraVardgivareMedSparr": {
            "minItems": 0,
            "type": "array"
          },
          "samtyckeFinns": {
            "minItems": 0,
            "type": "array",
            "items": {
              "vardgivareId": {
                "type": "string"
              },
              "vardgivareNamn": {
                "type": "string"
              },
              "includedInSjukfall": {
                "type": "boolean"
              }
            }
          },
          "samtyckeSaknas": {
            "minItems": 0,
            "type": "array"
          }
        },
        "type": "object",
        "additionalProperties": false,
        "required": [
          "vardenheterInomVGMedSparr",
          "andraVardgivareMedSparr"
        ]      },
      "sjukfallList": {
        "items": {
          "diagnos": {
            "properties": {
              "kapitel": {
                "type": [
                  "string",
                  "null"
                ]
              },
              "kod": {
                "type": [
                  "string",
                  "null"
                ]
              },
              "namn": {
                "type": [
                  "string",
                  "null"
                ]
              },
              "intygsVarde": {
                "type": "string"
              },
              "beskrivning": {
                "type": [
                  "string",
                  "null"
                ]
              }
            },
            "additionalProperties": false,
            "required": [
              "kapitel",
              "intygsVarde",
              "kod",
              "namn",
              "beskrivning"
            ]
          },
          "start": {
            "type": "string"
          },
          "slut": {
            "type": "string"
          },
          "dagar": {
            "type": "integer"
          },
          "intyg": {
            "minItems": 0,
            "type": "array",
            "items": {
              "vardgivareId": {
                "type": "string"
              },
              "vardgivareNamn": {
                "type": "string"
              },
              "vardenhetNamn": {
                "type": "string"
              },
              "vardenhetId": {
                "type": "string"
              },
              "patient": {
                "type": "object"
              },
              "diagnos": {
                "type": "object"
              },
              "bidiagnoser": {
                "type": "array"
              },
              "start": {
                "type": "string"
              },
              "slut": {
                "type": "string"
              },
              "signeringsTidpunkt": {
                "type": "string"
              },
              "dagar": {
                "type": "integer"
              },
              "grader": {
                "type": "array"
              },
              "lakare": {
                "type": "object"
              },
              "sysselsattning": {
                "type": "array"
              },
              "aktivtIntyg": {
                "type": "boolean"
              },
              "intygsId": {
                "type": "string"
              },
              "riskSignal": {
                "type": "object"
              },
              "otherVardgivare": {
                "type": "boolean"
              },
              "otherVardenhet": {
                "type": "boolean"
              },
              "additionalItems": false
            }
          }
        },
        "type" : "array",
        "additionalItems": false,
        "required": [
          "diagnos",
          "start",
          "slut",
          "dagar",
          "intyg"
        ]
      },
      "srsError": {
        "type": "boolean"
      }
    },
    "required": [
      "sjfMetaData",
      "sjukfallList",
      "srsError"
    ]
}